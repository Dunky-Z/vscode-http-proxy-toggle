{
  "name": "http-proxy-toggle",
  "displayName": "http-proxy-toggle",
  "description": "VS Codeの設定にあるHTTPプロキシ設定を簡単に切り替える拡張機能です",
  "publisher": "kita",
  "icon": "proxyIcon.png",
  "version": "0.0.3",
  "engines": {
    "vscode": "^1.89.0"
  },
  "categories": [
    "Other"
  ],
  "activation": {
    "property": "http",
    "command": "http-proxy-toggle.toggleProxy"
  },
  "activationEvents": [
    "*"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "configuration": {
      "title": "http-proxy-toggle",
      "properties": {
        "httpProxyToggle.home": {
          "type": "string",
          "description": "Home Http Proxy設定",
          "default": ""
        },
        "httpProxyToggle.office": {
          "type": "string",
          "description": "Office Http Proxy設定",
          "default": "http://proxy.example.com:80"
        }
      }
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/vscode": "^1.89.0",
    "@types/mocha": "^10.0.6",
    "@types/node": "18.x",
    "@typescript-eslint/eslint-plugin": "^7.7.1",
    "@typescript-eslint/parser": "^7.7.1",
    "eslint": "^8.57.0",
    "typescript": "^5.4.5",
    "@vscode/test-cli": "^0.0.9",
    "@vscode/test-electron": "^2.3.9"
  }
}
